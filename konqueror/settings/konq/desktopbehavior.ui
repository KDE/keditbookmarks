<connections>
    <connection>
        <sender>enableDevicesBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>devicesListView</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>autoLineupIconsBox</tabstop>
    <tabstop>showHiddenBox</tabstop>
    <tabstop>previewListView</tabstop>
    <tabstop>enableDevicesBox</tabstop>
    <tabstop>devicesListView</tabstop>
</tabstops>
